generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int         @id @default(autoincrement())
  loginId    String
  email      String
  name       String
  password   String
  createdAt  DateTime    @default(now())
  moneyLists MoneyList[]

}

model MoneyList {
  id     Int    @id @default(autoincrement())
  amount Int
  title  String
  date   String
  year   Int
  month  Int
  user   User?  @relation(fields: [userId], references: [id])
  userId Int?
}
